<UserControl x:Class="iS3.Client.ObjsViewGeneral"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:iS3.Client"
             mc:Ignorable="d" 
             xmlns:control="clr-namespace:iS3.Client.Controls;assembly=iS3.Client.Controls"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0">
            <control:IS3DataGrid Name="MyIS3DataGrid"/>
            <Grid Name="other" Visibility="Collapsed"></Grid>
        </Grid>
        <Grid Grid.Column="1">
            <control:ObjectView Name="MyObjectView"/>
        </Grid>
    </Grid>
</UserControl>
