<Window x:Class="iS3.Client.Controls.DGObjectsPropertyWin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:iS3.Client.Controls"
             mc:Ignorable="d" Title="对象组添加"
             d:DesignHeight="200" d:DesignWidth="300">
    <StackPanel Orientation="Vertical">
        <StackPanel Orientation="Horizontal">
            <TextBlock >Name:</TextBlock>
            <TextBox Width="100" Name="NameTB"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock>type:</TextBlock>
            <TextBox Width="100" Name="TypeTB"/>
        </StackPanel>
        <Button Width="100" Height="30" Name="SaveBtn" Click="SaveBtn_Click">确定</Button>
    </StackPanel>
</Window>
